material {
    name : customEffect,
    parameters : [
        {
            type : sampler2d,
            name : mainTexture
        },
        {
            type : sampler2d,
            name : guiTexture
        }
    ],
    requires : [ uv0 ],
    shadingModel : unlit,
    flipUV : false,
    blending : transparent
}

fragment {
    void material(inout MaterialInputs material) {
        prepareMaterial(material);
        vec2 uv = getUV0();
        vec4 color1 = texture(materialParams_mainTexture, uv);
        vec4 color2 = texture(materialParams_guiTexture, uv);

        material.baseColor = color1;
        if (color2.a > 0.0f) material.baseColor = color2;
    }
}
